{
  "name": "dvc/contao-custom-catalog",
  "version": "1.0.1",
  "description": "Custom Catalog (branches, products, persons) for Contao 5.3",
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "require": {
    "php": ">=8.1",
    "contao/core-bundle": "^5.3"
  },
  "autoload": {
    "psr-4": {
